	{\toggletrue{index:title:author}}
	{\togglefalse{index:title:author}}
  \ifstrequal{#1}{both}
	{\toggletrue{index:title:both}
	\toggletrue{index:title:author}}
	{\togglefalse{index:title:both}}
}
  
