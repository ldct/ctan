      \@thesistitle
    \end{center}
    \vspace{50mm}
    \mbox{} \hfill \begin{minipage}{67mm}
                     \begin{spacing}{1}
                     \@thesisapprovedby\hfill\mbox{} \\
                     \@thesiscommittee \\[15mm]
                     \end{spacing}
                     \setlength{\unitlength}{1mm}
                     \begin{picture}(67,70)(0,0)
                       \multiput(0,70)(0,-10)
                         {\thesiscommitteesize}{\line(1,0){67}}
                     \end{picture}
                   \end{minipage}
    \vfill \strut
    \end{spacing}
    \newpage}

